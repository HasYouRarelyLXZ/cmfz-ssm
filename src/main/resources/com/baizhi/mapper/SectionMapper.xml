<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.dao.SectionDao">

    <sql id="section_column">
        id,title,size,duration,uploadPath,uploadDate,album_id,times,oldname
    </sql>
    <insert id="add" parameterType="Section">

        INSERT INTO
        t_section
        VALUES (#{id},#{title},#{size},#{duration},#{uploadPath},#{uploadDate},#{album_id},#{times},#{oldname})
    </insert>
</mapper>

















